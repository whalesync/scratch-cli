{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to scratch server",
      "address": "127.0.0.1",
      "port": 9229,
      "enableContentValidation": true,
      "outputCapture": "std",
      "restart": true,
      "showAsyncStacks": true
    },
    {
      "name": "Debug Chat Server",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/pydantic-ai-agent/main.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/pydantic-ai-agent",
      "envFile": "${workspaceFolder}/pydantic-ai-agent/.env",
      "justMyCode": false,
      "python": "${workspaceFolder}/pydantic-ai-agent/.venv/bin/python",
      "gevent": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/pydantic-ai-agent"
      }
    }
  ]
}